   "source": [
    "pdp[\"invoice_currency\"].replace({\"USD\": 1, \"CAD\": 0.7}, inplace=True)\n",
    "pdp[\"converted_usd\"] = pdp[\"invoice_currency\"] * pdp[\"total_open_amount\"]\n",
    "pdp[\"invoice_currency\"].replace({1.0: \"USD\", 0.7: \"CAD\"}, inplace=True)\n",
    "pdp.head()"
   ]
